    "description": "Create Form from TwigTemplate and JSON configuration if necessary",
    "type": "library",
    "license": "CC-BY-SA-4.0",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "macwinnie",
            "email": "dev@macwinnie.me"
        }
    ],
    "require": {
        "twig/twig": "^3.3",
        "twig/extra-bundle": "^3.3",
        "macwinnie/regexfunctions": "^1.0"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "phpunit/phpunit": "^9.5",
        "friends-of-behat/mink-extension": "^2.5",
        "behat/mink-goutte-driver": "^1.2",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "autoload": {
        "psr-4" : {
            "macwinnie\\TwigForm\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "macwinnie\\TwigFormTests\\" : "tests/"
